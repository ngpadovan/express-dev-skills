<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(skills) { %>
        <li>
            <a href="/skills/<%= skills.id %>"><%= skills.language %></a>
            - 
            <%= skills.level !== 'advanced' ? 'can improve' : 'advanced' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>